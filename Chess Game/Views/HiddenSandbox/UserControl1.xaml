<UserControl x:Class="Chess_Game.Views.HiddenSandbox.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Chess_Game.Views.HiddenSandbox"
             xmlns:pieces="clr-namespace:Chess_Game.Views.Pieces"
             mc:Ignorable="d" 
             d:DesignHeight="65" d:DesignWidth="360"
             Background="Beige">
    <StackPanel Orientation="Horizontal" >
        <pieces:BishopUC Height="60" Width="60"/>
        <pieces:PawnUC Height="60" Width="60"/>
        <pieces:KnightUC Height="60" Width="60"/>
        <pieces:RookUC Height="60" Width="60"/>
        <pieces:QueenUC Height="60" Width="60"/>
        <pieces:kingUC Height="60" Width="60"/>
    </StackPanel>
</UserControl>
 