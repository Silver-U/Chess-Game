<UserControl x:Class="Chess_Game.Views.HiddenSandbox.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Chess_Game.Views.HiddenSandbox"
             xmlns:piecesLocation="clr-namespace:Chess_Game.Views.Pieces"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" >
        <piecesLocation:PawnUC/>
        <piecesLocation:BishopUC/>
        <piecesLocation:KnightUC/>
        <piecesLocation:RookUC/>
        <piecesLocation:QueenUC/>
        <piecesLocation:kingUC/>
    </StackPanel>
</UserControl>
